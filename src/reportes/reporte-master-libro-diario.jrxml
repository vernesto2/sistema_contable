		<queryString language="SQL">
			<![CDATA[select 
	pd.id, 
	pd.id_partida as id_partida, 
	c.nombre as cuenta, 
	folio_mayor, 
	parcial, 
	debe, 
	haber, 
	tipo_cargo_abono
	
from partida_detalle pd
inner join cuenta c on c.id = pd.id_cuenta
where pd.eliminado = false and c.eliminado = false
and pd.id_partida =   $P{param_id_partida}]]>
